{
    "name": "laravel-notification-channels/line-notify-channel",
    "description": "Use LINE Notify Service to notify users",
    "homepage": "https://github.com/laravel-notification-channels/line-notify-channel",
    "license": "MIT",
    "authors": [
        {
            "name": "hsuan",
            "email": "hsuan@hsuan.app",
            "homepage": "https://hsuan.app",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/notifications": "~8.0 || ~9.0 || ~10.0",
        "illuminate/support": "~8.0 || ~9.0 || ~10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\LINENotify\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\LINENotify\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\LINENotify\\LINENotifyServiceProvider"
            ],
            "aliases": {
                "LINENotify": "NotificationChannels\\LINENotify\\LINENotify"
            }
        }
    }
}
